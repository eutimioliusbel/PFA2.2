              "style": "simple",
              "explode": false,
              "schema": {
                 "type": "string",
                 "description": "keep session alive"
              }
            },
            {
              "name": "authenticationmode",
              "in": "header",
              "required": false,
              "style": "simple",
              "explode": false,
              "schema": {
                 "type": "string",
                 "description": "Authentication Mode"
              }
            }
            ],
            "requestBody": {
               "$ref": "#/components/requestBodies/GridData"
            },
            "responses": {
               "200": {
                  "description": "OK",
                  "content": {
                     "application/json": {
                         "schema": {
                            "$ref": "#/components/schemas/AddResponse"
                         }
                       }
