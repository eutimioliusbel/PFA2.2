{
  "name": "pfa-vanguard-backend",
  "version": "1.0.0",
  "description": "Backend API server for PFA Vanguard - Construction Equipment Tracking System",
  "main": "dist/server.js",
  "scripts": {
    "dev": "npm run prisma:seed && npx tsx init-pems-credentials.ts && tsx watch src/server.ts",
    "dev:no-seed": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:seed-full": "npm run prisma:seed && npx tsx init-pems-credentials.ts",
    "db:seed": "npm run prisma:seed-full",
    "test:load": "bash ../load-tests/run-all-tests.sh",
    "test:load:permission-check": "artillery run ../load-tests/permission-check.yml --output ../temp/output/permission-check-results.json",
    "test:load:permission-grant": "artillery run ../load-tests/permission-grant.yml --output ../temp/output/permission-grant-results.json",
    "test:load:org-switch": "artillery run ../load-tests/org-switch.yml --output ../temp/output/org-switch-results.json",
    "test:load:db-stress": "artillery run ../load-tests/db-stress.yml --output ../temp/output/db-stress-results.json",
    "test:load:memory": "npx tsx --expose-gc ../load-tests/memory-leak-test.ts",
    "test:load:generate-data": "npx tsx ../load-tests/generate-test-data.ts",
    "test:load:report": "npx tsx ../load-tests/generate-summary-report.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "pfa",
    "construction",
    "equipment",
    "tracking",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@aws-sdk/client-secrets-manager": "^3.940.0",
    "@dnd-kit/core": "^6.3.1",
    "@google/generative-ai": "^0.21.0",
    "@prisma/client": "^5.22.0",
    "@types/cron": "^2.0.1",
    "@types/ws": "^8.18.1",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cron": "^4.3.4",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "fastest-levenshtein": "^1.0.16",
    "json-logic-js": "^2.0.5",
    "jsonwebtoken": "^9.0.2",
    "mathjs": "^15.1.0",
    "node-fetch": "^2.7.0",
    "openai": "^6.9.1",
    "papaparse": "^5.5.3",
    "winston": "^3.17.0",
    "ws": "^8.18.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/benchmark": "^2.1.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitest/ui": "^4.0.14",
    "artillery": "^2.0.27",
    "artillery-plugin-expect": "^2.21.0",
    "autocannon": "^8.0.0",
    "benchmark": "^2.1.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "ioredis": "^5.8.2",
    "prettier": "^3.7.1",
    "prisma": "^5.22.0",
    "supertest": "^7.1.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.14"
  }
}
